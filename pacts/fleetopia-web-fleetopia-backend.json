{
  "consumer": {
    "name": "fleetopia-web"
  },
  "interactions": [
    {
      "description": "a request for all offers page 1",
      "request": {
        "method": "GET",
        "path": "/api/marketplace/all-offers",
        "query": {
          "limit": [
            "5"
          ],
          "page": [
            "1"
          ]
        }
      },
      "response": {
        "body": {
          "cargo": [
            {
              "id": "c_123",
              "price": 1850,
              "title": "Electronics Shipment",
              "urgency": "medium"
            }
          ],
          "pagination": {
            "limit": 5,
            "page": 1,
            "pages": 2,
            "total": 10
          }
        },
        "headers": {
          "content-type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.cargo": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.cargo[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.cargo[*].price": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.cargo[*].title": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.cargo[*].urgency": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.pagination": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.5.2"
    },
    "pactRust": {
      "ffi": "0.4.20",
      "models": "1.2.0"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "fleetopia-backend"
  }
}