{
  "name": "fleetopia",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "validate:db-logs": "node -e \"console.log('skip validate-db-logs for now')\""
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:unit": "jest",
    "test:e2e": "playwright test --reporter=dot",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static",
    "dev:backend": "pnpm --filter @fleetopia/backend run dev",
    "start:backend": "pnpm --filter @fleetopia/backend run start",
    "migrate": "pnpm --filter @fleetopia/backend run migrate:deploy",
    "gen:db-readme": "tsx scripts/gen-db-readme.ts",
    "contract:consumer": "jest __tests__/contracts --runTestsByPath __tests__/contracts/consumer.allOffers.pact.test.ts",
    "contract:provider": "jest __tests__/contracts --runTestsByPath __tests__/contracts/provider.verify.test.ts",
    "contract:test": "npm run contract:consumer && npm run contract:provider",
    "gen:db-readme": "tsx scripts/gen-db-readme.ts",
    "check:apis": "node scripts/smoke-apis.mjs"
  },
