    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.45.3",
    "@storybook/nextjs": "^8.2.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.2.1",
    "@types/react": "19.1.9",
    "autoprefixer": "^10.4.21",
