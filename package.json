    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
